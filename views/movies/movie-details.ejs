<!-- iteration 8 -->
<h2>Movie Details</h2>
<h2><%= movie.title %></h2>
<h2><%= movie.genre %></h2>
<h2><%= movie.plot %></h2>
<h2>Cast</h2>
<ul>
    <% movie.cast.forEach(cast => { %>
    <li>
        <h4>Name= <%= cast.name %></h4>
        <h4>Occupation= <%= cast.occupation %></h4>
        <h4>Catch Phrase= <%= cast.catchPhrase %></h4>
    </li>
    <% }) %>
</ul>
<form action="/movies/<%= movie._id %>/delete" method="post">
    <button type="submit"> Submit Form</button>
  </form>